{
  "name": "spiderman-marvel-app",
  "version": "1.0.0",
  "description": "A website containing Spiderman facts, abilities, and more using the Marvel API",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "vite build",
    "start": "node server/index.js",
    "test": "mocha tests/**/*.test.js",
    "test:watch": "mocha tests/**/*.test.js --watch",
    "test:setup": "NODE_ENV=test node -r dotenv/config tests/setup.js",
    "server": "nodemon server/index.js",
    "client": "vite",
    "lint": "eslint .",
    "format": "prettier --write .",
    "clean": "rm -rf node_modules && rm -rf dist",
    "preview": "vite preview"
  },
  "keywords": [
    "spiderman",
    "marvel",
    "react",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "chai": "^4.3.10",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.11",
    "vite": "^5.0.0"
  }
}
